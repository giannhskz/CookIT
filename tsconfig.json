{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/navbar.jsx", "app/user-information.jsx", "components/plzSign.jsx", "app/pantry/page.jsx", "app/pantry/session.jsx", "app/pantry/user.jsx", "app/pantry/table.jsx", "app/recipelist/page.jsx", "app/page.jsx", "pages/api/incredients/index.js", "lib/utils/index.jsx", "lib/prisma/users.jsx", "app/homepage/page.jsx"],
  "exclude": ["node_modules"]
}
